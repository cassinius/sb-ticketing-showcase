{
  "name": "ticket-system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bun run sb:gen:types && next dev",
    "build": "bun run sb:gen:types && next build",
    "start": "bun run sb:gen:types && next start",
    "lint": "next lint",
    "sb:gen:types": "npx supabase gen types typescript --schema public --local > supabase.ts",
    "sb:start": "npx supabase start",
    "sb:stop": "npx supabase stop"
  },
  "dependencies": {
    "@supabase/ssr": "^0.5.0",
    "@supabase/supabase-js": "^2.45.2",
    "next": "14.2.6",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "daisyui": "^4.12.10",
    "eslint": "^8",
    "eslint-config-next": "14.2.6",
    "postcss": "^8",
    "supabase": "^1.190.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
